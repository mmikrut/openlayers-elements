{
  "private": true,
  "name": "open-layers",
  "workspaces": [
    "demos",
    "elements/*"
  ],
  "scripts": {
    "start": "lerna run --scope demos start",
    "start:es5": "lerna run --scope demos start:es5",
    "build": "lerna run --no-private build",
    "pretest": "tslint -p tsconfig.json",
    "test": "echo 0",
    "analyze": "polymer analyze --sources elements/**/*.js > demos/analysis.json",
    "docs": "lerna run --scope demos build"
  },
  "devDependencies": {
    "lerna": "^3.13.0",
    "polymer-cli": "^1.9.6"
  }
}
